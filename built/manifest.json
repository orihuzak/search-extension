{ 
  "name": "Hello orihuzak",
  "description": "base level extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["bookmarks", "history", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "orihuzak-small.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
}
