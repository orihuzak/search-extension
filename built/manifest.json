{ 
  "name": "search tabs bookmarks history",
  "description": "search tabs bookmarks history",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": ["bookmarks", "history", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "img/orihuzak-small.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Open popup.html"
    },
    "close-tab": {
      "suggested_key": {
        "default": "Ctrl+D"
      },
      "description": "Close focused tab"
    }
  }
}
